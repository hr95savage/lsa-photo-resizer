{
  "version": 2,
  "builds": [
    {
      "src": "api/resize.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/resize",
      "dest": "/api/resize.py"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
