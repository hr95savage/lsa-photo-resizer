{
  "version": 2,
  "builds": [
    {
      "src": "api/resize.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/resize",
      "dest": "/api/resize.py"
    },
    {
      "src": "/(.*\\.(html|css|js))",
      "dest": "/public/$1"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    }
  ]
}
