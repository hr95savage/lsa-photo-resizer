{
  "version": 2,
  "routes": [
    {
      "src": "/api/resize",
      "dest": "/api/resize.py"
    },
    {
      "src": "/api/optimize",
      "dest": "/api/optimize.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*\\.(html|css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
