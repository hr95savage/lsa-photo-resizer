{
  "version": 2,
  "builds": [
    {
      "src": "api/resize.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/resize",
      "dest": "/api/resize.py"
    },
    {
      "src": "/(.*\\.(css|js))",
      "dest": "/$1"
    },
    {
      "src": "/",
      "dest": "/api/index.py"
    }
  ]
}
