{
  "version": 2,
  "builds": [
    {
      "src": "api/resize.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/resize",
      "dest": "api/resize.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "static/$1"
    },
    {
      "src": "/(.*\\.(html|css|js|png|jpg|jpeg|gif|svg|ico))",
      "dest": "static/$1"
    },
    {
      "src": "/",
      "dest": "static/index.html"
    }
  ]
}
